<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <Authors>Nick Dodd</Authors>
    <Company />
    <Product>AutoBogus</Product>
